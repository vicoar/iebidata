<% title "Lista de Ejemplares" %>

<h1><%= yield :title %></h1>

<table class="lista zebra">
  <tr>
  	<th>Nombre</th>
  	<th>Muestra</th>
    <th>Especie/Morfo</th>
  </tr>

<% @ejemplares.each do |ejemplar| %>
  <tr>
    <td><%=link_to ejemplar.nombre, ejemplar %></td>
    <td><%= ejemplar.muestra.nombre %></td>
    <td><%= ejemplar.taxon.nombre %></td>
  </tr>
<% end %>
</table>

<br />

<%= paginate @ejemplares %>