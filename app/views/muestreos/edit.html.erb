<% title "Editar Muestreo"%>

<h1><%= yield :title %></h1>

<%= form_for(@muestreo) do |f| %>

  <%= render :partial => "form", :locals => { :f => f } %>
  
  <p>
    <button type="submit" class="blue">Actualizar</button>
	<%= button_link 'Cancelar', @muestreo, "gray" %>
	<%= button_link 'Volver a la Lista', muestreos_path, "black" %>
  </p>
<% end %>