<% title "Lista de Muestreos"%>

<h1><%= yield :title %></h1>

<table class="lista zebra">
  <tr>
    <th>Nombre</th>
    <th>Fecha</th>
  </tr>

<% @muestreos.each do |muestreo| %>
  <tr>
    <td><%=link_to muestreo.nombre, muestreo %></td>
    <td><%= muestreo.fecha %></td>
  </tr>
<% end %>
</table>

<br />

<%= paginate @muestreos %>

<br />

<%= button_link 'Nuevo muestreo', new_muestreo_path, "green" %>