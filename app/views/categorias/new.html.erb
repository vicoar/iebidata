<% title "Nueva Categoria"%>

<h1><%= yield :title %></h1>

<%= form_for(@categoria) do |f| %>
  
  <%= render :partial => "form", :locals => { :f => f } %>
  
  <p>
    <button type="submit" class="green">Crear</button>
	<%= button_link 'Volver a Lista', categorias_path, "black" %>
  </p>
  
<% end %>

